        </ul>
        <ul class="nav green navbar-nav navbar-right hidden-xs">
          <li><%= link_to 'EX-25', ex25_path %></li>
        </ul>
      </div><!-- /.navbar-collapse -->
    </div><!-- /.container-fluid -->
  </nav>


<%= yield %>

 <br class="clr" />
